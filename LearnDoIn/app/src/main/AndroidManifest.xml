<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dudgns0507.learndoin" >

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:name=".Activity.model.UserInfo"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">

        <activity android:name=".Activity.activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Activity.activity.RegistrationActivity"></activity>
        <activity android:name=".Activity.activity.SettingActivity"></activity>
        <activity android:name=".Activity.activity.StudyActivity" android:noHistory="true"></activity>
        <activity android:name=".Activity.activity.FindUserActivity"></activity>
        <activity android:name=".Activity.activity.GraphActivity"></activity>
        <activity android:name=".Activity.activity.MainActivity"></activity>

    </application>
</manifest>